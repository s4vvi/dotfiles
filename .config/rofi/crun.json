[
    {
        "name": "Sandbox",
        "bin": "/usr/sbin/alacritty",
        "args": ["-e", "/bin/bash", "-c", "docker exec -it sandbox zsh -c tmux\\ has-session\\ \\&\\&\\ tmux\\ attach-session\\ \\|\\|\\ tmux\\ -2"]
    },
    {
        "name": "YouTube",
        "bin": "/usr/bin/firefox",
        "args": ["https://youtube.com"]
    },
    {
        "name": "HackTheBox - Home",
        "bin": "/usr/bin/firefox",
        "args": ["https://hackthebox.com"]
    },
    {
        "name": "HackTheBox - Labs",
        "bin": "/usr/bin/firefox",
        "args": ["https://app.hackthebox.com/home"]
    },
    {
        "name": "GitHub",
        "bin": "/usr/bin/firefox",
        "args": ["https://github.com"]
    },
    {
        "name": "Email",
        "bin": "/usr/bin/firefox",
        "args": ["https://mail.proton.me/u/0/inbox"]
    },
    {
        "name": "AVD - Mobile",
        "bin": "/opt/android-sdk/emulator/emulator",
        "args": ["-avd", "mobile"]
    },
    {
        "name": "AVD - Mobile (Writeable)",
        "bin": "/opt/android-sdk/emulator/emulator",
        "args": ["-avd", "mobile", "-writable-system"]
    },
    {
        "name": "AVD - Mobile Playstore",
        "bin": "/opt/android-sdk/emulator/emulator",
        "args": ["-avd", "mobile_playstore"]
    },
    {
        "name": "Logout",
        "bin": "/usr/bin/i3-msg",
        "args": ["exit"]
    },
    {
        "name": "Performance",
        "bin": "/usr/bin/asusctl",
        "args": ["profile", "-P", "Performance"]
    },
    {
        "name": "Balanced",
        "bin": "/usr/bin/asusctl",
        "args": ["profile", "-P", "Balanced"]
    },
    {
        "name": "Quiet",
        "bin": "/usr/bin/asusctl",
        "args": ["profile", "-P", "Quiet"]
    },
    {
        "name": "Alacritty (TMUX-less)",
        "bin": "/usr/bin/alacritty",
        "args": ["-e", "/usr/bin/zsh"]
    }
]
